#1769 Minimum Number of operations to move the all balls to each box
class Solution:
    def minOperations(self, b: str) -> List[int]:
        n = len(b)
        a = []
        for i in range(0,n):
            c = 0
            for j in range(0,n):
                if b[j]=="1":
                    c+=abs(j-i)
            a.append(c)
        return a
